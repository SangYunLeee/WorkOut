<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".WorkOutDetailListActivity"
    android:background="#4a6572"
    >


    <LinearLayout
        android:id="@+id/workout_container"
        android:layout_width="match_parent"
        android:layout_height="500sp"
        android:orientation="vertical"
        android:divider="@drawable/divider"
        android:showDividers="end|middle|beginning"
        >

        <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
            android:layout_width="match_parent"
            android:orientation="horizontal"
            android:layout_height="wrap_content"
            >

            <TextView
                android:layout_width="100sp"
                android:layout_height="50sp"
                android:layout_weight="1"
                android:text="Date"
                android:gravity="center"
                style="@style/detail_item_tab"
                />

            <TextView
                android:layout_width="100sp"
                android:layout_height="50sp"
                android:layout_weight="1"
                android:text="턱걸이"
                android:gravity="center"
                android:layout_gravity="center"
                style="@style/detail_item_tab"
                />

            <TextView
                android:layout_width="100sp"
                android:layout_height="50sp"
                android:layout_weight="1"
                android:text="푸시업"                android:gravity="center"
                android:layout_gravity="center"
                style="@style/detail_item_tab"
                />

            <TextView
                android:id="@+id/workout_item_of_tap3"
                android:layout_width="100sp"
                android:layout_height="50sp"
                android:layout_weight="1"
                android:text="스쿼트"
                android:gravity="center"
                style="@style/detail_item_tab"
                android:layout_gravity="center"
                />

        </LinearLayout>
    </LinearLayout>
</LinearLayout>